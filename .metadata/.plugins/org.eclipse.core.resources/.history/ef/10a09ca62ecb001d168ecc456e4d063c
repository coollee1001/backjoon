public class Main {
    public static void main(String[] args){
    	
    	System.out.printf("%-12s%2s%1s%\n", "NFC West", "W", "L", "T");
    	System.out.printf("-----------------------\n");
    	System.out.printf("%-12s%2s%1s%\n", "Seattle", "13", "3", "0");
    	System.out.printf("%-12s%2s%1s%\n", "San Francisco", "12", "4", "0");
    	System.out.printf("%-12s%2s%1s%\n", "Arizona", "10", "6", "0");
    	System.out.printf("%-12s%2s%1s%\n", "St. Louis", "7", "9", "0");
    	System.out.printf("\n");
    	System.out.printf("%-12s%2s%1s%\n", "NFC North", "W", "L", "T");
    	System.out.printf("-----------------------\n");
    	System.out.printf("%-12s%2s%1s%\n", "Green Bay", "8", "7", "1");
    	System.out.printf("%-12s%2s%1s%\n", "Chicago", "8", "8", "0");
    	System.out.printf("%-12s%2s%1s%\n", "Detroit", "7", "9", "0");
    	System.out.printf("%-12s%2s%1s%\n", "Minnesota", "5", "10", "1");
    	                   
    }
}